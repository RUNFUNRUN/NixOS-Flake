   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Twitch Clip Downloader",
   "permissions": [ "downloads" ],
   "short_name": "TCD",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.28"
}
