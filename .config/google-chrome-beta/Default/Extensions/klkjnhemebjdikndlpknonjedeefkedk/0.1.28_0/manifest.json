{
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "css": [ "badge.css" ],
      "js": [ "content.js" ],
      "matches": [ "*://*.twitch.tv/*" ]
   } ],
